{
  "name": "@lvce-editor/eslint-config-rules",
  "version": "0.0.0-dev",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-strip-types test/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "eslint": "^9.19.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.6",
    "execa": "^9.5.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  },
  "jest": {
    "injectGlobals": false,
    "testTimeout": 240000,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/fixtures/"
    ]
  }
}
