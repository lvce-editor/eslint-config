{
  "name": "e2e",
  "version": "0.0.0-dev",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch"
  },
  "keywords": [],
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.6",
    "execa": "^9.6.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4"
  },
  "jest": {
    "injectGlobals": false,
    "testTimeout": 240000,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/fixtures/"
    ]
  }
}
